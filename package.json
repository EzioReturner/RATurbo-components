{
  "name": "raturbo-components",
  "version": "0.1.4",
  "author": "zhev",
  "description": "A React UI Library.",
  "license": "MIT",
  "homepage": "https://github.com/EzioReturner/RATurbo-components",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib",
    "dist",
    "esm"
  ],
  "keywords": [
    "react",
    "UI",
    "component"
  ],
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EzioReturner/RATurbo-components"
  },
  "scripts": {
    "dev": "rimraf .docz && docz dev",
    "start": "npm run dev",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve",
    "clean": "rimraf lib esm dist",
    "pub": "npm run clean && npm run build && npm publish",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "build": "npm run clean && npm run build:types && gulp",
    "release": "ts-node ./scripts/release.ts",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "new": "plop --plopfile ./scripts/plopfile.ts",
    "postnew": "prettier --write components/**/*{ts,tsx} --loglevel silent",
    "lint-doc-comps": "prettier --write doc-comps/**/*.{tsx,ts} && eslint doc-comps/**/*.{tsx,ts} --fix && stylelint --syntax less doc-comps/**/*.less --fix",
    "lint-components": "prettier --write components/**/*{ts,tsx} && eslint components/**/*.{tsx,ts} --fix && stylelint --syntax less components/**/*.less --fix",
    "lint-fix": "yarn lint-doc-comps && yarn lint-components"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.170",
    "@types/react-highlight": "^0.12.2",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__react": "^9.1.2",
    "@types/vfile-message": "^2.0.0",
    "@umijs/fabric": "^1.2.12",
    "chalk": "^3.0.0",
    "commitizen": "^4.0.3",
    "cpr": "^3.0.1",
    "cz-conventional-changelog": "^3.0.2",
    "doctoc": "^1.4.0",
    "docz": "^2.3.2-alpha.0",
    "gatsby-plugin-import": "^2.1.5",
    "gatsby-plugin-less": "^3.0.17",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.0.4",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "lint-staged": "^9.5.0",
    "plop": "^2.6.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.0",
    "semver": "^7.3.2",
    "snapshot-diff": "^0.6.1",
    "through2": "^3.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.3",
    "react-highlight": "^0.14.0"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ],
    "doc-comps/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "doc-comps/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.9"
  }
}
